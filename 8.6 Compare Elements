#include <iostream>
using namespace std;

int main() {
    int rows, cols;

    cout << "Enter number of rows (at least 2): ";
    cin >> rows;
    cout << "Enter number of columns: ";
    cin >> cols;

    if (rows < 2) {
        cout << "Matrix must have at least 2 rows to compare.\n";
        return 1;
    }

    int matrix[100][100];

    cout << "Enter elements of the matrix:\n";
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            cin >> matrix[i][j];
        }
    }

    cout << "\nComparing elements of Row 1 and Row 2:\n";
    for (int j = 0; j < cols; j++) {
        if (matrix[0][j] == matrix[1][j]) {
            cout << "Element at column " << j << ": Equal (" << matrix[0][j] << ")\n";
        } else {
            cout << "Element at column " << j << ": Not Equal ("
                 << matrix[0][j] << " vs " << matrix[1][j] << ")\n";
        }
    }

    return 0;
}



/*     OUTPUT
Enter number of rows (at least 2): 3
Enter number of columns: 3
Enter elements of the matrix:
1
2
3
1
